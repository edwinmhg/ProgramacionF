program outputdata
  implicit none
  !Definimos las variables
  real, dimension(100) :: x, y, t
  real :: a, fi, fj
  integer :: i, h 
 real, parameter :: u = 10.0, g = 9.8, pi = 3.1415927
 integer, parameter :: maxangle = 90
 real, parameter :: deltat = .01
    open(1, file='Graficas.dat', status='unknown')

 do fj=0, maxangle, 15
    fj = float(j)
    do i=1, 100
       fi = float(i)
       i = t + fi * deltat
    a = h * pi / 180.0
     x(i) = u * i * cos(a)
     y(i) = u * i * sin(a) - 0.5 * g * i * i
     if (y(i).LT.0) exit

       
  write(1,*) x(i), y(i)
  write(1,*) ' '
  close(1)

  end do

  end do 

  end program outputdata
