program outputdata
  implicit none
  real, dimension(100) :: x, y, t
  real :: a
  integer :: i, h
 real, parameter :: u = 10.0, g = 9.8, pi = 3.1415927

    open(1, file='Graficas.dat', status='unknown')

 do h=0, 90, 15
    
     do i=1, 100 
    a = h * pi / 180.0
     x(i) = u * i * cos(a)
     y(i) = u * i * sin(a) - 0.5 * g * i * i
     if (y(i).LT.0) exit
  
     end do


       
  write(1,*) x(i), y(i)
  write(1,*) ' '
  close(1)

  end do 

  end program outputdata
