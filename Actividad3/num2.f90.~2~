program outputdata
  implicit none
  real, dimension(100) :: x, y
  real :: a, u, n
  integer :: i
 real, parameter ::  u = 10
  real, parameter :: g = 9.8
  ! setup x and y with some data, y angulo de 15 grados
  do i=1,100

     x(i) = u * i * cos(15)
     y(i) = u * i * sin(15) - 0.5 * g * i * i
  end do

  ! output data to a file
  open(1, file='Grafica15.dat', status='unknown')
  do i=1,100
     write(1,*) x(i), y(i)
  end do
  close(1)


  do i=1,100
      x(i) = u * i * cos(30)
      y(i) = u * i * sin(30) - 0.5 * g * i * i

  end do

  ! output data to a file
  open(1, file='Grafica30.dat', status='unknown')
  do i=1,100
     write(1,*) x(i), y(i)
  end do
  close(1)


  do i=1,100
     x(i) = u * i * cos(45)
     y(i) = u * i * sin(45) - 0.5 * g * i * i

  end do

  ! output data to a file
  open(1, file='Grafica45.dat', status='unknown')
  do i=1,100
     write(1,*) x(i), y(i)
  end do
  close(1)


  
 do i=1,100
     x(i) = u * i * cos(60)
     y(i) = u * i * sin(60) - 0.5 * g * i * i

  end do

  ! output data to a file
  open(1, file='Grafica60.dat', status='unknown')
  do i=1,100
     write(1,*) x(i), y(i)
  end do
  close(1)

  
  
 do i=1,100
     x(i) = u * i * cos(75)
     y(i) = u * i * sin(75) - 0.5 * g * i * i

  end do

  ! output data to a file
  open(1, file='Grafica75.dat', status='unknown')
  do i=1,100
     write(1,*) x(i), y(i)
  end do
  close(1)


  
  do i=1,100
     x(i) = u * i * cos(90)
     y(i) = u * i * sin(90) - 0.5 * g * i * i

  end do

  ! output data to a file
  open(1, file='Grafica90.dat', status='unknown')
  do i=1,100
     write(1,*) x(i), y(i)
  end do
  close(1)
  
end program outputdata
