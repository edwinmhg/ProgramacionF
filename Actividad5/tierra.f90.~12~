!Declaramos variables

program begin

  implicit none
  double precision :: a, fi
  double precision, parameter :: D=1.496d8, t=365.26d0         
  real, parameter :: pi=3.1416d0
  integer :: i
  double precision, dimension(1000) :: x, y
  
  a = a * pi / 180
  
  !Calculamos la posicion.
  open (1, file ='datos.dat', status = 'unknown')

  do i=1, 360, 1
     fi = float(i)

     x(i) = D * dcos(fi)
     y(i) = D * dsin(fi)

     write (1,*) x(i), y(i)
     write (1,*) ' '
     
  end do
  close (1)
  
  end program
