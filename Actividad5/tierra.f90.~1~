  !Declaramos variables
  program begin
  double precision, parameter :: D=149600000, T=365.26          
  real, parameter :: pi=3.1416
  real :: 
  !Calculamos la circunferencia (trayectoria de la tierra)
  C = 2 * pi * (D/2)
  w = 2 * pi / T
  do i=1,T
     t=float(i)
     teta= 2 * pi * t /T
     




  end program
